<div class="page_header">
	<h1>Atendentes</h1>
</div>

<div class="inside">
	
	<div class="topo">
		<%= form_tag administrator_chat_atendentes_path, :method => :get, :remote => true do %>
		<div class="yui3-g">
			<div class="yui3-u-1-3">
				<div class="field">
					<%= label_tag :nome %>
					<%= text_field_tag :nome %>
				</div>
			</div>
			<div class="yui3-u-1-3">
				<div class="field">
					<%= label_tag :email %>
					<%= text_field_tag :email %>
				</div>
			</div>
			<div class="yui3-u-1-3">
				<div class="field">
					<label>&nbsp;</label>
					<%= submit_tag 'Buscar', :class => 'button' %>
				</div>
			</div>
		</div>
		<% end %>
	</div>
	
  <div id="atendentes-index">
    <%= render "chat_atendentes" %>
  </div>
</div>
