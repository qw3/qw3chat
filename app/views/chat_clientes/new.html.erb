<% if Rails.application.config.assets.enabled %>
	<%= javascript_include_tag 'templates/qw3chat/lib/chat.status.js' %>
<% else %>
	<%= javascript_include_tag 'qw3chat/chat.status.js' %>
<% end %>

<div class="logo">
	<%= image_tag Settings['QW3CHAT.logo'] %>
</div>

<div class="page_header">
	<h1>Aqui online</h1>
</div>

<div class="inside">
	
	<div class="topo">
		<div class="texto-descritivo">
			<%= (ChatSession.atendimento_online?) ? Settings['QW3CHAT.texto_online'] : Settings['QW3CHAT.texto_offline'] %>
		</div>
	
		<div id="chat_status">
			<%= render '/administrator/chat_sessions/status' %>
		</div>
		<div class="clear"></div>
	</div>
	
	<%= render 'form' %>
</div>