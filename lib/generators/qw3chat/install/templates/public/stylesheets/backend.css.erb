div#leftbar ul.menu li a.qw3chat-chat-leftbar {
	background: url("<%= asset_path 'lib/qw3chat/chat.png' %>") no-repeat scroll 10px padding-box transparent;
}

div#leftbar ul.menu li a.qw3chat-configuracao-atendimento-leftbar {
	background: url("<%= asset_path 'lib/qw3chat/configuracao.png' %>") no-repeat scroll 10px padding-box transparent;
}

a.cliente, a.encerrar {
	padding: 5px;
	font-size: 10pt;
}

a.cliente {
	background-color: #a8ca92;
}
a.cliente:hover {
	background-color: #bfdaae;
}

a.encerrar {
	background-color: #c2a6a6;
}
a.encerrar:hover {
	background-color: #d5b7b7;
}

div.atrasada {
	background-color: #ddacac;
	border-color: red;
	border-width: 2px;
}

div#welcome {
    float: right;
    margin: 10px;
    text-align: right;
    width: 700px;
}


div#welcome .welcome, div#welcome .logout {
	display:inline;
	margin-left: 5px;
}

div.buttonmargin {
	margin: 30px 0;
}

div#welcome div.ponto {
	float:left;
	width: 60%;
}

div.ponto input[type="submit"] {
    border-radius: 5px 5px 5px 5px;
    color: #FFFFFF;
    cursor: pointer;
    font-size: 13px;
    font-weight: bold;
    margin-top: 10px;
    padding: 8px;
    text-shadow: 0 0 1px #222222;
    text-transform: uppercase;
}

div.ponto .aberto input[type="submit"] {
	background: #008000; /* Old browsers */
	background: -moz-linear-gradient(top, #008000 0%, #02a500 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#008000), color-stop(100%,#02a500)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #008000 0%,#02a500 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #008000 0%,#02a500 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #008000 0%,#02a500 100%); /* IE10+ */
	background: linear-gradient(top, #008000 0%,#02a500 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#008000', endColorstr='#02a500',GradientType=0 ); /* IE6-9 */

	border: 1px solid #008000;
}

div.ponto .fechado input[type="submit"] {
	background: #b21f24; /* Old browsers */
	background: -moz-linear-gradient(top, #b21f24 0%, #ff0000 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#b21f24), color-stop(100%,#ff0000)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #b21f24 0%,#ff0000 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #b21f24 0%,#ff0000 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #b21f24 0%,#ff0000 100%); /* IE10+ */
	background: linear-gradient(top, #b21f24 0%,#ff0000 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#b21f24', endColorstr='#ff0000',GradientType=0 ); /* IE6-9 */

	border: 1px solid #B21F24;
}

div.chat {
	box-shadow: 0 0 3px #CCC;
	-moz-box-shadow: 0 0 3px #CCC;	
	-webkit-box-shadow: 0 0 3px #CCC;	

	border-radius: 6px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;

	border: 1px solid #D7D7D7;
	padding: 10px;
	margin-bottom: 10px;
}

div.chat div.cliente {
	font-size: 18px;
	float:left;
	width: 70%;
}

div.chat div.cliente_email {
	font-size: 14px;
	float:left;
	width: 70%;
}

div.chat div.cliente_telefone {
	font-size: 14px;
	float:left;
	width: 30%;
}

div.chat div.data {
	width: 30%;
	float:left;
	font-size: 11px;
}

div.chat div.mensagem {
	float:left;
	width: 100%;
	margin: 10px;
	font-style: italic;
}

div.chat div.status {
	float:left;
	width: 25%;
	font-size: 11px;
	font-family:"Lucida Grande";
}

div.chat div.status span {
	font-weight: bold;
	font-size: 14px;
	display:block;
}

div.chat ul.botoes {
	margin:0;padding:0;
	float:right;
	width: 75%;
	text-align:right;
}

div.chat ul.botoes li {
	display:inline-block;
	margin: 0 20px;
	height: 60px;
	float:right;
	font-family:"Lucida Grande";
}

div.chat ul.botoes li a {
	display:inline-block;
	height: 60px;
}

div.chat ul.botoes li a span {
	position:relative;
	top: 48px;
}

div.chat ul.botoes li.atender a {
	background:url("<%= asset_path 'lib/qw3chat/atendimento/atender.png' %>") no-repeat scroll center top;
}

div.chat ul.botoes li.finalizar a {
	background:url("<%= asset_path 'lib/qw3chat/atendimento/finalizar.png' %>") no-repeat scroll center top;
}

div.chat ul.botoes li.visualizar a {
	background:url("<%= asset_path 'lib/qw3chat/atendimento/visualizar.png' %>") no-repeat scroll center top;
}

div.historico-mensagens span {
	font-weight: bold;
}

div#chat-notificacao {
	display:block;
	position:fixed;
	bottom: 0;
	right: 0;
	background: #fff;
	-webkit-box-shadow: 0 0 3px #a2a2a2;
	-moz-box-shadow: 0 0 3px #a2a2a2;
	box-shadow: 0 0 3px #a2a2a2;
	z-index: 20;
	text-align:center;
}

div#chat-notificacao .notificacao h1 {
	background: #FF8A00;
	color: #fff;
	font-weight: normal;
	font-size: 16px;
	margin: 0;
	padding: 10px;
}

div#chat-notificacao .notificacao .inside {
	text-align:center;
}

div#chat-notificacao .notificacao .inside div {
	margin: 10px 0;
}

div#chat-notificacao .notificacao a {
	margin-bottom: 10px;
	display:inline-block;
	background: url("<%= asset_path 'lib/qw3chat/icons/atendimentos-mini.png' %>") no-repeat;
}

div#chat-notificacao .notificacao a span {
	padding-left: 25px;
	color: #000;
	font-size: 14px;
}